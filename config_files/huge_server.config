server {
    port 8080;
    host 127.0.0.1;


    client_max_body_size 100000000;

    default_server true;

    location / {
        allow_methods GET POST;

        root /;

        autoindex on;
    }
}

server {
    port 8081;
    host 127.0.0.1;


    client_max_body_size 100000000;

    default_server false;

    location / {
        allow_methods GET POST;

        root /;

        autoindex on;

        index index.html;

    }
}

